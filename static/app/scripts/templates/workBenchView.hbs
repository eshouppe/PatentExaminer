<div class="mdl-color--white mdl-shadow--2dp mdl-cell mdl-cell--12-col mdl-grid workBenchView">
  <div class="mdl-cell mdl-cell--12-col">
    <div class="mdl-grid">
      <div class="mdl-cell mdl-cell--6-col mdl-cell--8-col-tablet mdl-cell--4-col-phone">
        <div class="headerContainer">
          <h5>Workbench</h5>
          <p>Saved patents from previous results</p>
        </div>
        <div class="mdl-cell mdl-cell--12-col listContainer">
          <ul class="demo-list-two mdl-list">
            {{#each savedPatents}}
              <li class="mdl-list__item mdl-list__item--two-line">
              <span class="mdl-list__item-primary-content">
                <i class="material-icons mdl-list__item-avatar">lightbulb_outline</i>
                <span>{{this.patentId}}</span>
                <span class="mdl-list__item-sub-title">{{this.patentTitle}}</span>
              </span>
              </li>
            {{/each}}
          </ul>
        </div>
      </div>
      <div class="mdl-cell mdl-cell--6-col mdl-cell--8-col-tablet mdl-cell--4-col-phone">
        <div class="headerContainer">
          <h5>Previous Searches</h5>
          <p>Select a search to reload</p>
        </div>
        <div class="mdl-cell mdl-cell--12-col listContainer">
          <ul class="demo-list-two mdl-list">
            {{#each previousSearch}}
              <li class="mdl-list__item mdl-list__item--two-line">
                <span class="mdl-list__item-primary-content">
                  <i class="material-icons mdl-list__item-avatar">search</i>
                  <span>{{this.searchId}}</span>
                  <span class="mdl-list__item-sub-title">{{this.searchText}}</span>
                </span>
              </li>
            {{/each}}
          </ul>
        </div>
      </div>
    </div>
  </div>
</div>
